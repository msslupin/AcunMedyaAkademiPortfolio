@model AcunMedyaAkademiPortfolio.Models.TblProject
@{
    ViewBag.Title = "CreateProject";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}

<h3>Yeni Proje Oluştur</h3>
<form method="post">
    <label>Projenin Adı</label>
    <input type="text" class="form-control" name="ProjectName" />

    <label>Projenin Resmi</label>
    <input type="text" class="form-control" name="ProjectImageUrl" />

    <label>Projenin Kategorisi</label>
    <input type="text" class="form-control" name="ProjectCategory" />

    <br /><br />
    <button class="btn btn-info">Kategoriyi Kaydet</button>
</form>

